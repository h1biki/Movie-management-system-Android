<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal">


    <TextView
        android:id="@+id/tv_movieName_watchlist"
        android:layout_width="136dp"
        android:layout_height="120dp"
        android:textAppearance="?android:attr/textAppearanceMedium"/>

    <TextView
        android:id="@+id/tv_releaseDate_watchlist"
        android:layout_width="136dp"
        android:layout_height="120dp"
        android:textAppearance="?android:attr/textAppearanceMedium"/>
    <TextView
        android:id="@+id/tv_adddate_watchlist"
        android:layout_width="136dp"
        android:layout_height="120dp"
        android:textAppearance="?android:attr/textAppearanceMedium"/>

    <TextView
        android:id="@+id/tv_addtime_watchlist"
        android:layout_width="136dp"
        android:layout_height="120dp"
        android:textAppearance="?android:attr/textAppearanceMedium"/>

    <TextView
        android:id="@+id/tv_watchid_watchlist"
        android:layout_width="136dp"
        android:layout_height="120dp"
        android:textAppearance="?android:attr/textAppearanceMedium"/>

    <TextView
        android:id="@+id/tv_movieid_watchlist"
        android:layout_width="136dp"
        android:layout_height="120dp"
        android:textAppearance="?android:attr/textAppearanceMedium"/>

</LinearLayout>
